<%= form_for ([@character, @special]) do |f| %>

  <div class="field">
   <%= f.label :Name %><br />
   <%= f.text_field :special_name %>
 </div>
  <div class="field">
    <%= f.label :Description %><br />
    <%= f.text_area :special_desc %>
  </div>
  <div class="field">
    <%= f.label :Benefit %><br />
    <%= f.text_area :special_benefit %>
  </div>
  <div class="field">
    <%= f.label :Notes %><br />
    <%= f.text_area :special_notes %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

<%= link_to 'Back', character_path(@character)  %>

